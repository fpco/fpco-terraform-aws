export ENVIRONMENT=<<<SETME>>>
export NAMESPACE=kops-vpc.dev-sandbox.fpcomplete.com
export CLUSTER_NAME=$ENVIRONMENT.$NAMESPACE

export TF_VAR_name_prefix=$ENVIRONMENT
export TF_VAR_region=us-east-2
export TF_VAR_availability_zones=us-east-2a,us-east-2b,us-east-2c
export TF_VAR_kubernetes_cluster_name=$CLUSTER_NAME
export TF_VAR_cluster_admin_keyname=$CLUSTER_NAME-admin
export TF_VAR_vpc_cidr=192.168.0.0/16
export TF_VAR_public_subnet_cidrs=192.168.10.0/24,192.168.20.0/24,192.168.30.0/24